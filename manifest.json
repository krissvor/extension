{
  "manifest_version": 2,

  "name": "Session helper",
  "description": "Opens all the tabs for a certain purpose.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "https://ajax.googleapis.com/",
    "storage"
  ]
}